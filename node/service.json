{
  "memory": 256,
  "ttl": 60,
  "timeout": 40,
  "minReplicas": 30,
  "maxReplicas": 100,
  "workers": 4,
  "events": {
    "onAppInstalled": {
      "sender": "apps",
      "keys": [
        "setup"
      ]
    }
  },
  "routes": {
    "createList": {
      "path": "/app/add/products/:email",
      "public": true
    },
    "getFacets": {
      "path": "/app/retrieve/facets/:email",
      "public": true
    },
    "getProducts": {
      "path": "/app/retrieve/products/:email",
      "public": true
    },
    "removeProducts": {
      "path": "/app/remove/products/:email",
      "public": true
    },
    "ping": {
      "path": "/_v/private/app/mylist/ping",
      "public": true
    }
  }
}
